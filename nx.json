{
  "extends": "@nrwl/workspace/presets/npm.json",
  "npmScope": "medea",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "lint", "package", "prepare"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "@medea/client": {
      "implicitDependencies": [],
      "dependsOn": [
        {
          "target": "build",
          "projects": ["dependencies", "devDependencies"]
        }
      ]
    },
    "@medea/server": {
      "implicitDependencies": [],
      "dependsOn": [
        {
          "target": "build",
          "projects": ["dependencies", "devDependencies"]
        }
      ]
    },
    "@medea/types": {
      "implicitDependencies": []
    },
    "@medea/utils": {
      "implicitDependencies": []
    },
    "@medea/models": {
      "implicitDependencies": []
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
